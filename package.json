{
  "engines": {
    "node": ">=14.16.x <15",
    "npm": ">=6.14.11 <7"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.16.0",
    "@mermaid-js/mermaid-cli": "^8.13.4",
    "@parcel/babel-preset-env": "2.0.1",
    "@parcel/optimizer-cssnano": "2.0.1",
    "@parcel/optimizer-htmlnano": "2.0.1",
    "@parcel/packager-css": "2.0.1",
    "@parcel/packager-html": "2.0.1",
    "@parcel/packager-raw-url": "2.0.1",
    "@parcel/packager-xml": "^2.0.1",
    "@parcel/resolver-glob": "^2.0.1",
    "@parcel/transformer-css": "2.0.1",
    "@parcel/transformer-html": "2.0.1",
    "@parcel/transformer-image": "^2.0.1",
    "@parcel/transformer-postcss": "2.0.1",
    "@parcel/transformer-posthtml": "2.0.1",
    "@parcel/transformer-webmanifest": "2.0.1",
    "@parcel/transformer-xml": "^2.0.1",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-plugin-compat": "^4.0.0",
    "jsdom": "^18.1.1",
    "mocha": "^9.1.3",
    "mocha-param": "^2.0.1",
    "nyc": "^15.1.0",
    "parcel": "^2.0.1",
    "parcel-config-precache-manifest": "0.0.4",
    "parcel-optimizer-ogimage": "^0.1.0",
    "parcel-plugin-sw-asset-urls": "^0.1.2",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "parcel-resolver-ignore": "^2.0.0",
    "playwright": "^1.16.3",
    "postcss": "^8.4.1",
    "prettier": "^2.5.0",
    "st": "^3.0.0"
  },
  "dependencies": {
    "core-js": "3.19.2",
    "dayjs": "^1.10.7",
    "jsdom-global": "^3.0.2",
    "localforage": "1.7.3",
    "navigo": "7.1.2",
    "pikaday": "^1.8.2",
    "timeago.js": "4.0.2"
  },
  "scripts": {
    "test": "mocha --color --require @babel/register src/scripts/tests/",
    "test:coverage": "nyc npm run test",
    "test-integration": "mocha --color --require @babel/register --bail --timeout 0 src/scripts/tests/integration/",
    "test-integration:ci": "mocha --color --require @babel/register --bail --timeout 15000 --retries=5 src/scripts/tests/integration/",
    "lint": "eslint --color src/scripts/ src/service-worker.js",
    "prebuild": "rm -rf dist",
    "build": "parcel build src/index.html src/*.html --detailed-report 20",
    "build-dev": "parcel src/*.html --log-level info --open"
  },
  "parcelIgnore": [
    "version.json"
  ],
  "browserslist": [
    ">0.5% in FR",
    "last 2 versions",
    "IE 11",
    "Firefox ESR",
    "not dead"
  ]
}
